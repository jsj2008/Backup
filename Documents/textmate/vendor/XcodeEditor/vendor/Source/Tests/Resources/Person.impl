#include <iostream>

using namespace std;

class Person {

public:

    Person(string name, int age) {

        this->name = name;

        this->age = age;

    }

    string getName() {

        return name;

    }

    int getAge() {

        return age;

    }

private:

    string name;

    int age;

};


